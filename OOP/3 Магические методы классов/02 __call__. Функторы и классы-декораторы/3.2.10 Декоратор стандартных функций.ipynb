   "source": [
    "class InputDigits:\n",
    "    def __init__(self, func):\n",
    "        self.__func = func\n",
    "        \n",
    "    def __call__(self):\n",
    "        return list(map(int, self.__func().split()))\n",
    "\n",
    "@InputDigits\n",
    "def input_dg():\n",
    "    return input()\n"
   ]
