   "source": [
    "class Point:\n",
    "    def __init__(self, x, y):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "    \n",
    "    def clone(self):\n",
    "        obj = super().__new__(Point)\n",
    "        obj.__dict__ = self.__dict__.copy()\n",
    "        return obj\n",
    "\n",
    "\n",
    "\n",
    "pt = Point(2, 3)\n",
    "pt_clone = pt.clone()"
   ]
  }
